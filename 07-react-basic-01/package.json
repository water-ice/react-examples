{
  "name": "react-basic",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:client": "cross-env PATH_ENV=client npm run serve",
    "build:client": "cross-env PATH_ENV=client npm run dist:client",
    "dev:manage": "cross-env PATH_ENV=manage npm run serve",
    "build:manage": "cross-env PATH_ENV=manage npm run dist:manage",
    "serve": "npm run lint && node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config  webpack.config.dev.js --color --progress --open --hot",
    "dist:client": "npm run remove:dist:client && npm run lint && webpack --config webpack.config.dist.js -p --progress --color",
    "dist:manage": "npm run remove:dist:manage && npm run lint && webpack --config webpack.config.dist.js -p --progress --color",
    "remove:dist:client": "rm -rf ./client/dist",
    "remove:dist:manage": "rm -rf ./manage/dist",
    "lint": "esw webpack.config.* app",
    "lint:watch": "npm run lint -- --watch"
  },
  "author": "deot",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.8.3",
    "antd-mobile": "^1.0.7",
    "babel-polyfill": "^6.23.0",
